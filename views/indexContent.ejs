<% productData.forEach((element) => { %>
    <div class="indexPContainer">
        <img src="images/<%= element.image %>" class="indexPImage">
        <h2 class="indexPTitle"><%= element.name %></h2>
        <p class="indexPDescription"><%= element.summary %></p>
        <p class="indexPPrice">DKK <%= element.price %>,-</p>
        <a class="indexPBtn" href="/produkter?id=<%= element.id %>">Info</a>
    </div>
<% }) %>
