<html lang="en"></html>
<html>
    <head>
        <meta charset="UTF-8">
        <title><% if(productData == null) { %>
            Produkt
        <% } else if (productData.length == 1) { %>
            <% productData.forEach((element) => { %>
                <%= element.name %>
            <% }) %>
        <% } else { %>
            Alle produkter
        <% } %>
        </title>
        <link rel='stylesheet' href='stylesheets/style.css'/>
    </head>
    <body>
        <%- include header %>
        <div id="wrapper">
            <div id="wrapperAlign">
                <%- include headerLogo %>
                <main>
                    <aside id="pageAside">
                        <%- include asideNav %>
                        <%- include asideSubIndex %>
                    </aside>
                    <div id="dynamicContent">
                        <%- include productContent %>
                    </div>
                </main>
                <%- include footer %>
            </div>
        </div>
    </body>
</html>